					<Add option="-std=c++0x" />
					<Add option="-W" />
					<Add option="-fPIC" />
					<Add option="-DNOPCH" />
					<Add option="-fexceptions" />
					<Add option="-Os" />
				</Compiler>
				<ExtraCommands>
					<Add after="$(CPDE_USR)/bin/cpde_usr -project=$(PROJECT_NAME)  -root=$(PROJECT_DIR)  -build=$(TARGET_NAME)  -target=$(TARGET_OUTPUT_FILE)  -usr=$(CPDE_USR)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
